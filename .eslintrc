{
  "parser": "babel-eslint",
  "ecmaFeatures": {},
  "plugins": [],
  "rules": {
    /* POSSIBLE ERRORS */
    "no-cond-assign": 2,
    "no-console": [1, {
      "allow": [
        "debug",
        "error",
        "groupCollapsed",
        "groupEnd",
        "info",
        "warn"
      ]
    }],
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 2,
    "no-empty": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": 0,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-prototype-builtins": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-unsafe-finally": 2,
    "use-isnan": 2,
    "valid-jsdoc": 0,
    "valid-typeof": 2,


    /* BEST PRACTICES */
    "accessor-pairs": 0,
    "array-callback-return": 2,
    "block-scoped-var": 2,
    "complexity": [0, 20],
    "consistent-return": 2,
    "curly": 2,
    "default-case": 2,
    "dot-notation": [2, { "allowKeywords": true }],
    "eqeqeq": [2, "allow-null"],
    "guard-for-in": 2,
    "dot-location": [2, "property"],
    "no-alert": 2,
    "no-caller": 2,
    "no-case-declarations": 2,
    "no-div-regex": 2,
    "no-else-return": 2,
    "no-empty-function": [2, {
      "allow": ["functions", "methods"]
    }],
    "no-empty-pattern": 2,
    "no-eq-null": 0,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-label": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-implicit-coercion": [0, {
      "boolean": false,
      "number": true,
      "string": true,
      "allow": []
    }],
    "no-implicit-globals": 0,
    "no-implied-eval": 2,
    "no-invalid-this": 0,
    "no-iterator": 2,
    "no-labels": [2, { "allowLoop": false, "allowSwitch": false }],
    "no-lone-blocks": 2,
    "no-magic-numbers": [2, {
      "ignore": [],
      "ignoreArrayIndexes": true,
      "enforceConst": true,
      "detectObjects": false
    }],
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-octal-escape": 2,
    "no-octal": 2,
    "no-param-reassign": [2, { "props": false }],
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-assign": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-throw-literal": 2,
    "no-unmodified-loop-condition": 0,
    "no-unused-expressions": [2, { "allowShortCircuit": false, "allowTernary": false }],
    "no-unused-labels": 2,
    "no-useless-call": 0,
    "no-useless-concat": 2,
    "no-useless-escape": 2,
    "no-void": 2,
    "no-warning-comments": [0, { "terms": ["todo", "fixme", "xxx"], "location": "start" }],
    "no-with": 2,
    "radix": 2,
    "vars-on-top": 2,
    "wrap-iife": [2, "inside"],
    "yoda": 2,


    /* STRICT MODE */
    "strict": 2,


    /* VARIABLES */
    "no-catch-shadow": 2,
    "no-delete-var": 2,
    "no-label-var": 2,
    "no-restricted-globals": 0,
    "no-shadow-restricted-names": 0,
    "no-shadow": 0,
    "no-undef-init": 2,
    "no-undef": 2,
    "no-undefined": 0,
    "no-unused-vars": 0,
    "no-use-before-define": 0,


    /* STYLE */
    "array-bracket-spacing": 0,
    "block-spacing": 2,
    "brace-style": [2, "1tbs", { "allowSingleLine": true }],
    "camelcase": 0,
    "comma-dangle": 0,
    "comma-spacing": 2,
    "comma-style": 0,
    "computed-property-spacing": 0,
    "consistent-this": 0,
    "eol-last": 0,
    "func-names": 0,
    "func-style": 0,
    "id-blacklist": 0,
    "id-length": 0,
    "id-match": 0,
    "indent": 0,
    "key-spacing": 0,
    "keyword-spacing": 2,
    "linebreak-style": 0,
    "lines-around-comment": 0,
    "max-depth": 0,
    "max-len": 0,
    "max-lines": 0,
    "max-nested-callbacks": 0,
    "max-params": 0,
    "max-statements": 0,
    "max-statements-per-line": 0,
    "multiline-toernary": 0,
    "new-parens": 0,
    "newline-after-var": 0,
    "newline-before-return": 0,
    "newline-per-chained-call": 0,
    "new-cap": [2, { "newIsCap": true, "properties": false }],
    "no-array-constructor": 2,
    "no-bitwise": 0,
    "no-continue": 0,
    "no-lonely-if": 0,
    "no-mixed-operators": 0,
    "no-mixed-spaces-and-tabs": 0,
    "no-multiple-empty-lines": 0,
    "no-negated-condition": 0,
    "no-nested-ternary": 0,
    "no-new-object": 0,
    "no-plusplus": 0,
    "no-restricted-syntax": 0,
    "no-whitespace-before-property": 0,
    "no-spaced-func": 0,
    "no-tabs": 0,
    "no-ternary": 0,
    "no-trailing-spaces": 0,
    "no-underscore-dangle": 0,
    "no-unneeded-ternary": 0,
    "object-curly-newline": 0,
    "object-curly-spacing": [2, "always"],
    "object-property-newline": 0,
    "quotes": [2, "single", {"avoidEscape": true, "allowTemplateLiterals": true}],
    "quote-props": 0,
    "one-var-declaration-per-line": 0,
    "one-var": 0,
    "operator-assignment": 0,
    "operator-linebreak": 0,
    "padded-blocks": 0,
    "semi": [2, "always"],
    "semi-spacing": [2, { "before": false, "after": true }],
    "sort-vars": 0,
    "space-before-blocks": 2,
    "space-before-function-paren": 2,
    "space-in-parens": [2, "never"],
    "space-infix-ops": 2,
    "space-unary-ops": 0,
    "spaced-comment": 0,
    "unicode-bom": 0,
    "wrap-regex": 0
  },


  "globals": {
    "Blob": false,
    "ngInject": false,
    "window": false,
    "moment": false,
    "angular": false,
    "require": false,
    "console": false,
    "module": false,
    "document": false,
    "coordutils": false,
    "describe": false,
    "it": false,
    "xit": false,
    "inject": false,
    "beforeEach": false,
    "expect": false,
    "setTimeout": false,
    "File": false
  }
}
